\r\n            let editForm = document.getElementsByClassName(\"editForm\")\r\n            editForm[0].removeAttribute(\"hidden\");\r\n\r\n            let taskToSave = document.getElementsByClassName(\"taskToSave\");\r\n            taskToSave[0].value = result.data.content;\r\n\r\n            let saveTask = document.getElementsByClassName(\"saveTask\");\r\n            saveTask[0].addEventListener(\"click\", function (event) {\r\n                let data = {\r\n                    content: taskToSave[0].value\r\n                };\r\n                requests.sendRequest(`/tasks/${result.data.id}`, \"PUT\", data)\r\n            });\r\n        })\r\n        .catch((err) => {\r\n            console.log(`This is an error from retreiving a record from db: ${err}`)\r\n        })\r\n}"},"lineCount":null}},"error":null,"hash":"028306d86e4442b5be7072e4e0e553a0","cacheData":{"env":{}}}